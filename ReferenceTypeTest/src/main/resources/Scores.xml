<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"> 

<bean id="sc" class="com.student.Scores">
<property name="physics" value="80"></property>
<property name="chemistry" value="90"></property>
<property name="maths" value="85"></property>
</bean>

<bean id="st" class="com.student.Student">
<property name="score" ref="sc">

</property>
</bean>


</beans>    